class Solution {
public:
    double trimMean(vector<int>& arr) {
        int n=arr.size();
        sort(arr.begin(),arr.end());
        double ans=0;
        int pert = n/20;
        for(int i=pert;i<n-pert;i++){
            ans += arr[i];
        }
        ans /= (n-2*pert);
        return ans;
    }
};
