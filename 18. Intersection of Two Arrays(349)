class Solution {
public:
    vector<int> intersection(vector<int>& a, vector<int>& b) {
        set<int> s,ans;
        vector<int> v;
        for(int i=0;i<a.size();i++){
            s.insert(a[i]);
        }
        for(int i=0;i<b.size();i++){
            if(s.find(b[i])!=s.end()){
                ans.insert(b[i]);
            }
        }
        for(auto itr=ans.begin();itr!=ans.end();itr++){
            v.push_back(*itr);
        }
        return v;
    }
};
