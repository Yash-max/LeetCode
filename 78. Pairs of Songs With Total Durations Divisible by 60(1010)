class Solution {
public:
    int numPairsDivisibleBy60(vector<int>& time) {
        int ans = 0;
        vector<int> v(60,0);
        for(int i=0;i<time.size();i++){
            v[time[i] % 60]++;
        }
        for(int i = 1;i<30;i++){
            ans += v[i] * v[60-i];
        }
        if(v[0] > 0){
            ans += ((v[0])*(v[0]-1))/2;
        }
        if(v[30] > 0){
            ans += ((v[30])*(v[30]-1))/2;
        }
        return ans;
    }
};
