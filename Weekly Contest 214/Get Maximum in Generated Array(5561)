class Solution {
public:
    int getMaximumGenerated(int n) {
        if(n <= 1){
            return n;
        }
        vector<int> v(n+1,0);
        int ans=1;
        v[0]=0,v[1]=1;
        for(int i=2;i <= n;i++){
            if(i % 2 == 0){
                v[i] = v[i/2];
            }else{
                int x = (i-1)/2;
                v[i] = v[x]+v[x+1];
            }
            ans = max(ans,v[i]);
        }
        return ans;
    }
};
