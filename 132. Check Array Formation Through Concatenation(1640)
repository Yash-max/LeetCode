class Solution {
public:
    bool canFormArray(vector<int>& arr, vector<vector<int>>& pieces) {
        int n = arr.size();
        int p = pieces.size();
        for(int i=0;i<n;i++){
            int flag = 0;
            for(int j=0;j<p;j++){
                if(arr[i] == pieces[j][0]){
                    flag = 1;
                    for(int k=0;k<pieces[j].size();k++){
                        if(arr[i+k] != pieces[j][k]){
                            return false;
                        }else{cout<<pieces[j][k]<<" ";}
                    }
                    i += pieces[j].size()-1;
                    break;
                }
            }
            if(flag == 0) return false;
        }
        return true;
    }
};
